<p>Create New Song</p>

<%= form_with scope: :song, url: songs_path, local: true do |form| %>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>

<% if user_signed_in? %>
<%= link_to 'New Song', new_song_path %>
<h1>REEEE</h1>
<%= link_to 'log out', destroy_user_session_url, method: :delete %>

<% end %>

<%= link_to 'Back', songs_path %>
